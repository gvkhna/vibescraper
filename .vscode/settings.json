    {
      "invalid": "\\u2006",
      "message": "Replace SIX-PER-EM SPACE with space",
      "valid": " "
    },
    {
      "invalid": "\\u2007",
      "message": "Replace FIGURE SPACE with space",
      "valid": " "
    },
    {
      "invalid": "\\u2008",
      "message": "Replace PUNCTUATION SPACE with space",
      "valid": " "
    },
    {
      "invalid": "\\u2009",
      "message": "Replace THIN SPACE with space",
      "valid": " "
    },
    {
      "invalid": "\\u200A",
      "message": "Replace HAIR SPACE with space",
      "valid": " "
    },
    {
      "invalid": "\\u200B",
      "message": "Replace ZERO WIDTH SPACE with space",
      "valid": " "
    },
    {
      "invalid": "\\u202F",
      "message": "Replace NARROW NO-BREAK SPACE with space",
      "valid": " "
    },
    {
      "invalid": "\\u205F",
      "message": "Replace MEDIUM MATHEMATICAL SPACE with space",
      "valid": " "
    },
    {
      "invalid": "\\u3000",
      "message": "Replace IDEOGRAPHIC SPACE with space",
      "valid": " "
    },
    {
      "invalid": "\\uFEFF",
      "message": "Replace ZERO WIDTH NO-BREAK SPACE with space",
      "valid": " "
    },
    {
      "invalid": "\\uE0020",
      "message": "Replace TAG SPACE with space",
      "valid": " "
    }
  ]
}
