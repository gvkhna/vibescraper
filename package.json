{
  "name": "vibescraper",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "bun": "please-use-pnpm",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "scripts": {
    "build": "bun turbo run build",
    "clean": "bun turbo run clean && rimraf dist .turbo node_modules/.cache *.tsbuildinfo",
    "dev": "bun turbo run dev --filter=@vibescraper/website",
    "dev:all": "bun turbo run dev",
    "lint": "bun turbo run lint",
    "pnpm:nuke": "pnpm -r exec rimraf node_modules && rimraf pnpm-lock.yaml",
    "pnpm:upgrade-version": "pnpm uninstall corepack;pnpm install -D corepack;corepack prepare pnpm@latest --activate;corepack use pnpm@latest",
    "test": "bun turbo run test",
    "test:integration": "bun turbo run test --filter=@vibescraper/integration-tests",
    "typecheck": "bun turbo run typecheck"
  },
