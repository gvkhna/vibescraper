{
  "name": "@vibescraper/dev-utils",
  "type": "module",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "clean": "rimraf dist .turbo node_modules/.cache node_modules/.vite-temp *.tsbuildinfo",
    "dev": "vite build --watch",
    "format": "bun eslint --quiet --config ../../eslint.config.mjs . --fix && bun prettier --write . --ignore-path ../../.prettierignore",
    "lint": "bun eslint --config ../../eslint.config.mjs .",
    "typecheck": "bun tsc --noEmit && bun eslint --quiet ."
  },
  "devDependencies": {
    "@types/node": "24.5.2"
  }
}
