{
  "name": "@vibescraper/integration-tests",
  "private": true,
  "description": "Integration tests for the Vibescraper website",
  "type": "module",
  "scripts": {
    "clean": "rimraf --glob dist .turbo node_modules/.cache node_modules/.vite* *.tsbuildinfo",
    "format": "bun eslint --quiet --config ../../eslint.config.mjs . --fix && bun prettier --write .",
    "lint": "bun eslint --config ../../eslint.config.mjs .",
    "test": "vitest run",
    "typecheck": "bun tsc --noEmit"
  },
  "devDependencies": {},
  "peerDependencies": {
    "@types/node": "*",
    "vite": "*",
    "vitest": "*"
  }
}
