{
  "name": "@vibescraper/json-tree-table",
  "type": "module",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "clean": "rimraf --glob dist .turbo node_modules/.cache node_modules/.vite* *.tsbuildinfo",
    "dev": "vite build --watch",
    "format": "bun eslint --quiet --config ./eslint.config.mjs . --fix && bun prettier --write . --ignore-path ../../.prettierignore",
    "lint": "bun eslint --config ./eslint.config.mjs .",
    "storybook": "bun ./node_modules/storybook/dist/bin/index.js dev -p 6006",
    "storybook:build": "bun ./node_modules/storybook/dist/bin/index.js build",
    "typecheck": "bun tsc --noEmit && bun eslint --quiet --config ./eslint.config.mjs ."
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "npm:@types/web@latest"
  },
  "peerDependencies": {}
}
