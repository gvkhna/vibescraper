{
  "name": "@vibescraper/scraping-tests",
  "private": true,
  "description": "Tests for example scrapers",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist .turbo node_modules/.cache node_modules/.vite-temp *.tsbuildinfo",
    "format": "bun eslint --quiet --config ../../eslint.config.mjs . --fix && bun prettier --write .",
    "lint": "bun eslint --config ../../eslint.config.mjs .",
    "test": "vitest run",
    "typecheck": "bun tsc --noEmit && bun eslint --quiet ."
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@vibescraper/html-processor": "workspace:*",
    "@vibescraper/json-schemas": "workspace:*",
    "@vibescraper/sandbox": "workspace:*"
  }
}
