{
  "name": "@vibescraper/website",
  "type": "module",
  "browserslist": [
    "defaults and fully supports es6-module, not op_mini all"
  ],
  "scripts": {
    "build": "bun --env-file=../../.env astro build",
    "clean": "rimraf --glob .astro dist .turbo node_modules/.cache node_modules/.vite* *.tsbuildinfo",
    "db:generate": "bun --env-file=../../.env ./node_modules/drizzle-kit/bin.cjs generate",
    "db:migrate": "bun --env-file=../../.env ./node_modules/drizzle-kit/bin.cjs migrate",
    "db:seed": "bun --env-file=../../.env src/db/seed.ts",
    "dev": "bun --env-file=../../.env astro dev",
    "drizzle-kit": "bun --env-file=../../.env ./node_modules/drizzle-kit/bin.cjs",
    "format": "bun eslint --quiet --config ./eslint.config.mjs . --fix && bun prettier --write .",
    "lint": "bun eslint --config ./eslint.config.mjs .",
    "maildev": "maildev -v --hide-extensions STARTTLS --mail-directory tmp/maildev/",
    "preview": "bun --env-file=../../.env astro preview",
    "start": "bun --env-file=../../.env dist/server/entry.mjs",
    "storage:reset": "find storage -mindepth 1 ! -name '.keep' -exec rm -rf {} +",
    "storybook": "bun ./node_modules/storybook/dist/bin/index.js dev -p 6006",
    "storybook:build": "bun ./node_modules/storybook/dist/bin/index.js build",
    "test:coverage": "bun --env-file=../../.env vitest run --coverage",
    "test:vitest": "bun --env-file=../../.env vitest run",
    "typecheck": "bun astro check --minimumSeverity error && bun tsc --noEmit && bun eslint --quiet --config ./eslint.config.mjs .",
    "typecheck:full": "bun tsc --noEmit --pretty --noErrorTruncation",
    "worker": "bun src/worker/standalone.ts"
  },
