---
import MainLayout from './main-layout.astro'
export interface Props {
  title: string
}
const { title } = Astro.props
---

<MainLayout {title}>
  <main>
    <div class='relative'>
      <div
        class='relative px-4 py-16 sm:px-6 sm:py-24 lg:mx-auto lg:max-w-7xl lg:px-8
          lg:py-32'
      >
        <div class=''>
          <div class='mx-auto max-w-md sm:max-w-4xl'>
            <slot />
          </div>
        </div>
      </div>
    </div>
  </main>
</MainLayout>
