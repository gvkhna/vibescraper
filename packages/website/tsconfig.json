{
  "compilerOptions": {
    "baseUrl": ".",
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "jsx": "preserve",
    "noErrorTruncation": true,
    "paths": {
      "@/*": ["src/*"]
    },
    "types": []
  },
  "exclude": ["dist", "node_modules", "src/**/_*.ts", "src/**/_*.tsx", "src/**/*tmp*.ts", "src/**/*tmp*.tsx"],
  "extends": "../../tsconfig.base.json",
  "include": [
    "types.d.ts",
    ".astro/types.d.ts",
    "drizzle.config.ts",
    "src/**/*.astro",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/env.d.ts"
  ]
}
